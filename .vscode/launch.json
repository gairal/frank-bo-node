{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "attach",
      "port": 9229,
      "request": "attach",
      "restart": true,
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "args": ["-b", "-i", "-u", "--verbose", "${relativeFile}"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "neverOpen",
      "name": "jest",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "request": "launch",
      "smartStep": true,
      "type": "node"
    },
    {
      "args": ["${workspaceFolder}/scripts/importDatastore.ts"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "/home/dro/com-gairal-frank-OLD.json"
      },
      "name": "import-datastore",
      "request": "launch",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/tsnd",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "args": ["${workspaceFolder}/scripts/exportFirestore.ts"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "name": "export-firestore",
      "request": "launch",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/tsnd",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    }
  ]
}
