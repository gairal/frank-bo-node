{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "attach",
      "port": 9229,
      "request": "attach",
      "restart": true,
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "args": ["-b", "-i", "-u", "--verbose", "${relativeFile}"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "neverOpen",
      "name": "jest",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "request": "launch",
      "smartStep": true,
      "type": "node"
    }
  ]
}
